#include <iostream>
using namespace std;
int main() {
    double num1, num2;
    cout << "Enter the first number: ";
    cin >> num1;
    cout << "Enter the second number: ";
    cin >> num2;
    string result = (num1 == num2) ? "Equal" : "Not Equal";
    cout << "The numbers are " << result << endl;
    return 0;
}

#include <iostream>
using namespace std;
int main() {
    int rows;
    cout << "Enter the number of rows: ";
    cin >> rows;
    for (int i = 1; i <= rows; ++i) {
        // Print stars
        for (int j = 1; j <= i; ++j) {
            if (j == i && (i == 3 || i == 4))
                cout << "*";
            else
                cout << "*";
        }
        cout << endl;
    }
    return 0;
}
